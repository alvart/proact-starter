{
  "dependencies": {
    "create-react-class": "^15.6.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "bower": "^1.8.2",
    "cross-env": "^5.1.0",
    "enzyme": "^3.1.0",
